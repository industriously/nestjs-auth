{
  "name": "@devts/nestjs-auth",
  "version": "1.2.2",
  "description": "Oauth2 Auth Module to NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "ISC",
  "scripts": {
    "type-coverage": "type-coverage --detail",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prebuild": "ts-patch install -s",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json"
  },
  "devDependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "~5.48.1",
    "@typescript-eslint/parser": "~5.48.1",
    "eslint": "8.31.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.4",
    "ts-patch": "^2.1.0",
    "type-coverage": "^2.24.1",
    "typescript": "~4.9.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-industriously:industriously/nestjs-auth.git"
  },
  "bugs": {
    "url": "https://github.com/industriously/nestjs-auth/issues"
  },
  "keywords": [
    "nestjs-auth",
    "nest-auth",
    "auth",
    "Oauth2",
    "passport",
    "authentication"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 99,
    "project": "tsconfig.json",
    "strict": true,
    "ignoreAsAssertion": true,
    "ignoreFiles": [
      "lib/**/*.test.ts",
      "dist/**/*",
      "dist/*"
    ]
  }
}
